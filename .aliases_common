alias gb='git branch'
alias gst='git status'
alias gfe='git fetch -p'
alias gco='git checkout'
alias gdi='git diff'
alias gpul='git pull'

alias t='tig'
alias v='vim'

alias ll='ls -al'
alias mkdir='mkdir -p'
alias cd.='cd ..'

alias ghub='hub browse'
alias gh='ghq list | peco | cut -d "/" -f 2,3 | xargs hub browse'
